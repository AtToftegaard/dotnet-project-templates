{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "MessageDeserializationConfig": {
    "Assemblies": [
      "Area.Domain.Domain"
    ]
  },
  "ODIS": {
    "connectionString": "mongodb://mongodb:mongodb@127.0.0.1",
    "databaseName": "ODIS"
  }
}
